{
  "name": "@24wings/plugins-shopxo-supplier",
  "version": "0.1.8",
  "exports": {
    "./plugin": "./plugin.ts"
  },
  "tasks": {
    "dev": "deno run --watch -A --unstable-raw-imports dev.tsx",
    "publish": "deno publish --no-check --allow-slow-types"
  },
  "license": "MIT",
  "imports": {
    "@24wings/build": "jsr:@24wings/build@^0.0.1",
    "@std/assert": "jsr:@std/assert@1",
    "@std/path": "jsr:@std/path@^1.1.4",
    "hono": "npm:hono@^4.11.4",
    "preact": "npm:preact@^10.28.2",
    "preact-iso": "npm:preact-iso@^2.11.1",
    "preact-render-to-string": "npm:preact-render-to-string@^6.6.5"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "preact"
  },
  "publish": {
    "include": [
      "src",
      "dev.tsx",
      "plugin.ts",
      "deno.json",
      "README.md"
    ]
  }
}